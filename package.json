{
  "name": "refunk",
  "version": "3.0.0-0",
  "description": "Simple functional setState for React",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "babel src -d dist",
    "start": "x0 dev docs/App.js -op 9000",
    "build": "x0 build docs/App.js -d docs",
    "cover": "nyc report --reporter=html",
    "test": "nyc ava"
  },
  "keywords": [
    "react",
    "state",
    "functional",
    "hoc"
  ],
  "author": "Brent Jackson",
  "license": "MIT",
  "devDependencies": {
    "@compositor/x0": "^2.0.0-8",
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "nyc": "^11.2.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-test-renderer": "^16.3.0"
  },
  "dependencies": {
    "prop-types": "^15.5.10"
  },
  "ava": {
